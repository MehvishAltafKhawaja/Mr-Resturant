@{
}
@model List<Intern>


<h1>Your Profile</h1>

<table>


    <thead>
       <tr>
        
            <th>ID</th>
            <th>NAME</th>
            <th>EMAIL</th>
            <th>PHONENUMBER</th>
            <th>ADDRESS</th>


       </tr>
    </thead>
    <tbody>
        @foreach (var Intern in Model)

        {
            <tr>
                <td>@Intern.Id</td>
                <td>@Intern.Name</td>
                <td>@Intern.Email</td>
                <td>@Intern.PhoneNumber</td>
                <td>@Intern.Address</td>
            </tr>
        <td>
            <a class="btn btn-primary" asp-controller="Page"
               asp-action="Edit" asp-route-id="@Intern.Id">
                Edit Details
            </a>

            <a class="btn btn-danger" asp-controller="Page"
               asp-action="Delete" asp-route-id="@Intern.Id">
                Delete Intern
            </a>

        </td>
        }

    </tbody>

</table>